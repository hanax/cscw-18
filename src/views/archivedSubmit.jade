doctype html
html
	head
		title Submit - CSCW 2018
		include shared-head

	body

	include shared-small-header

	.section.main
		.column.main-left-menu
			ul
				each n in archivedSubmitContents.items
					a(href='#'+n.title)
						li(id='menu-item-'+n.title)!= n.title
		.column.main-right-content
			each n in archivedSubmitContents.items
				.content-item
					.title(id=n.title)= n.title
					.separator
						span.fa.fa-circle
						span.fa.fa-circle
						span.fa.fa-circle
					.content!= n.content

	include shared-footer
